CREATE TABLE bdefileretrieval.lvl1_prdmfils (
    loan_num text,
    segment_no text,
    sg_owner_seq_no int,
    sg_partc_cd text,
    sg_p_d_seq_no int,
    lps_cyc_dt timestamp,
    client_no text,
    cret_dt timestamp,
    cret_user_id text,
    delta_file_byte text,
    rec_stat text,
    sg_p_d_adj_am decimal,
    sg_p_d_b_in_rcv_ba decimal,
    sg_p_d_begin_dt timestamp,
    sg_p_d_begin_null text,
    sg_p_d_day_qt int,
    sg_p_d_e_in_rcv_ba decimal,
    sg_p_d_end_dt timestamp,
    sg_p_d_end_null text,
    sg_p_d_int_am decimal,
    sg_p_d_int_coll_am decimal,
    updt_dt timestamp,
    updt_user_id text,
    PRIMARY KEY (loan_num, segment_no, sg_owner_seq_no, sg_partc_cd, sg_p_d_seq_no, lps_cyc_dt)
) WITH CLUSTERING ORDER BY (segment_no ASC, sg_owner_seq_no ASC, sg_partc_cd ASC, sg_p_d_seq_no ASC, lps_cyc_dt DESC)
    AND bloom_filter_fp_chance = 0.01
    AND caching = '{"keys":"ALL", "rows_per_partition":"NONE"}'
    AND comment = ''
    AND compaction = {'class': 'org.apache.cassandra.db.compaction.SizeTieredCompactionStrategy'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND dclocal_read_repair_chance = 0.1
    AND default_time_to_live = 0
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';

