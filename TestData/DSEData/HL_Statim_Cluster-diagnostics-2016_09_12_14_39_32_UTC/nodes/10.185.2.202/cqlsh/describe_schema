    dpi_bkrpty_chngd_id text,
    dpi_bkrpty_ind text,
    dpi_claims_cd text,
    dpi_claims_chngd_dt timestamp,
    dpi_claims_chngd_dt_null text,
    dpi_claims_chngd_id text,
    dpi_evictn_cd text,
    dpi_evictn_chngd_dt timestamp,
    dpi_evictn_chngd_dt_null text,
    dpi_evictn_chngd_id text,
    dpi_fclsr_ind text,
    dpi_fclsr_ind_chngd_dt timestamp,
