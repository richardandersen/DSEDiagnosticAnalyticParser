CREATE TABLE capitalonehomeloans.homebase_he_underwriting_view (
    lnkey text,
    ukey int,
    cross_apply_seq int,
    app_date timestamp,
    appn_dt timestamp,
    aprsl_in_lc_ts timestamp,
    asset_expiration_dt timestamp,
    cltv decimal,
    credit_expiration_dt timestamp,
    curr_gfe_acptnc_ts timestamp,
    curr_mailbox text,
    curr_mailbox_ts timestamp,
    curr_uw_ts timestamp,
    doc_lvl_34_cnt int,
    dti decimal,
    estimated_closing_dt timestamp,
    exception_flag text,
    first_brwr_bankrela_cd int,
    first_brwr_last_nm text,
    hfs_hfi_type text,
    income_expiration_dt timestamp,
    income_verify_ts timestamp,
    last_dcsn_ts timestamp,
    last_fileto_evt text,
    last_fileto_evt_ts timestamp,
    last_uw_dcsn text,
    lc_name text,
    lien text,
    lo_name text,
    loan_amt decimal,
    loan_closing_dt timestamp,
    loan_closing_dt_keydate timestamp,
    loan_product text,
    loan_purpose text,
    lob text,
    lob_workflowtab text,
    ls_name text,
    mgttab_loanlvl_select int,
    mi_required text,
    pending_cond_cnt int,
    pending_reworks_cnt int,
    pending_reworks_type text,
    prop_state text,
    pti decimal,
    rework_comment text,
    rework_reason text,
    rework_strt_ts timestamp,
    second_sign_type text,
    self_empl_flag text,
    site text,
    snap_dt timestamp,
    tab text,
    task text,
    task_type text,
    team text,
    to_closing_days int,
    uw_name text,
    uw_tat_bd int,
    workflowtab_loanlvl_select int,
    workflowtab_reworklvl_select int,
    worknum_return_ts timestamp,
    workout_renewal_indicator text,
    PRIMARY KEY (lnkey, ukey, cross_apply_seq)
) WITH CLUSTERING ORDER BY (ukey ASC, cross_apply_seq ASC)
    AND bloom_filter_fp_chance = 0.01
    AND caching = '{"keys":"ALL", "rows_per_partition":"NONE"}'
    AND comment = ''
    AND compaction = {'class': 'org.apache.cassandra.db.compaction.SizeTieredCompactionStrategy'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND dclocal_read_repair_chance = 0.1
    AND default_time_to_live = 0
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';

