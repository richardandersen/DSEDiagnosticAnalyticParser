
CREATE KEYSPACE foundation WITH replication = {'class': 'NetworkTopologyStrategy', 'us-east_01': '3', 'us-west-2_02': '3'}  AND durable_writes = true;

CREATE TABLE foundation.evacd_geo_call_type (
    calltypeid decimal PRIMARY KEY,
    changestamp decimal,
    customerdefinitionid decimal,
    dbflags decimal,
    deleted text,
    description text,
    enterprisename text,
    snap_dt timestamp
) WITH bloom_filter_fp_chance = 0.01
    AND caching = '{"keys":"ALL", "rows_per_partition":"NONE"}'
    AND comment = ''
    AND compaction = {'class': 'org.apache.cassandra.db.compaction.SizeTieredCompactionStrategy'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND dclocal_read_repair_chance = 0.1
    AND default_time_to_live = 0
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';

CREATE TABLE foundation.evacd_geo_agent (
    skilltargetid decimal,
    load_dt timestamp,
    load_dt_ timestamp,
    agentdesksettingsid decimal,
    agentstatetrace text,
    changestamp decimal,
    configparam text,
    deleted text,
    description text,
    enterprisename text,
    firstname text,
    lastname text,
    password_r text,
    peripheralid int,
    peripheralname text,
    peripheralnumber decimal,
    scheduleid decimal,
    snap_dt timestamp,
    temporaryagent text,
    PRIMARY KEY (skilltargetid, load_dt, load_dt_)
) WITH CLUSTERING ORDER BY (load_dt ASC, load_dt_ ASC)
    AND bloom_filter_fp_chance = 0.01
    AND caching = '{"keys":"ALL", "rows_per_partition":"NONE"}'
    AND comment = ''
    AND compaction = {'class': 'org.apache.cassandra.db.compaction.SizeTieredCompactionStrategy'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND dclocal_read_repair_chance = 0.1
    AND default_time_to_live = 0
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';

CREATE TABLE foundation.evacd_geo_skill_group (
    skilltargetid decimal,
    changestamp decimal,
    availableholdoffdelay int,
    baseskilltargetid decimal,
    configparam text,
    dbflags decimal,
    deleted text,
    description text,
    enterprisename text,
    extension text,
    peripheralid int,
    peripheralname text,
    peripheralnumber decimal,
    priority int,
    scheduleid decimal,
    snap_dt timestamp,
    subgroupmasktype int,
    subskillgroupmask text,
    PRIMARY KEY (skilltargetid, changestamp)
) WITH CLUSTERING ORDER BY (changestamp ASC)
    AND bloom_filter_fp_chance = 0.01
    AND caching = '{"keys":"ALL", "rows_per_partition":"NONE"}'
    AND comment = ''
    AND compaction = {'class': 'org.apache.cassandra.db.compaction.SizeTieredCompactionStrategy'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND dclocal_read_repair_chance = 0.1
    AND default_time_to_live = 0
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';

