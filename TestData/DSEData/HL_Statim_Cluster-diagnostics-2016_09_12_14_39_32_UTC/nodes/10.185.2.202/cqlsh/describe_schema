CREATE TABLE hl_data_commons.l3_heloc_fraud_score (
    loan_num text PRIMARY KEY,
    create_date timestamp,
    extra_trees_score double,
    forest_score double,
    gbm_score double,
    logistic_score double,
    rf_score double,
    trigger_info map<text, text>,
    update_date timestamp,
    xgb_score double
) WITH bloom_filter_fp_chance = 0.01
    AND caching = '{"keys":"ALL", "rows_per_partition":"NONE"}'
