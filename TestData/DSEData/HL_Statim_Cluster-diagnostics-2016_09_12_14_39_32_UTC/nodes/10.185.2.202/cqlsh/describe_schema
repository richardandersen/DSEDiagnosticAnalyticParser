    svc_id decimal,
    snap_dt timestamp,
    strt_dt timestamp,
    abndd_in_que_call_cnt int,
    abndd_que_calls_dlay_scnd_dur int,
    abndd_within_svc_lvl_cnt int,
    agt_hndl_scnd_dur int,
    auto_out_call_cnt int,
    auto_out_call_hndl_scnd_dur int,
    auto_out_call_hold_cnt int,
    auto_out_call_hold_scnd_dur int,
    auto_out_call_talk_scnd_dur int,
    avg_agt_hndl_scnd_dur int,
    avg_dlay_abndd_que_scnd_dur int,
    avg_dlay_scnd_dur int,
    avg_talk_scnd_dur int,
    avg_wait_tm_scnd_dur int,
    calls_with_svc_lvl_cnt int,
    edw_publn_id decimal,
    frc_clsd_call_cnt int,
    gmt_tzn_ofst_mi_dur int,
    ib_call_cnt int,
    icm_svc_lvl_met_call_cnt int,
    icm_svc_lvl_val decimal,
    max_abndd_que_call_scnd_dur int,
    max_dlay_scnd_dur int,
    ob_call_cnt int,
    ofrd_call_cnt int,
    ovflw_in_call_cnt int,
    ovflw_out_call_cnt int,
    prphl_svc_lvl_met_call_cnt int,
    prphl_svc_lvl_ofrd_call_cnt int,
    prphl_svc_lvl_val decimal,
    shrt_call_cnt int,
    shrt_call_scnd_dur int,
    strt_tm timestamp,
    strt_ts timestamp,
    tot_agt_ansd_call_cnt int,
    tot_calls_in_que_cnt int,
    tot_dlay_scnd_dur int,
    tot_hndld_call_cnt int,
    tot_hold_call_scnd_dur int,
    tot_icm_rted_call_cnt int,
    tot_talk_scnd_dur int,
    tot_wait_scnd_dur int,
    trfr_in_call_cnt int,
    trfr_out_call_cnt int,
    trmntd_call_cnt int,
    PRIMARY KEY (svc_id, snap_dt, strt_dt)
) WITH CLUSTERING ORDER BY (snap_dt ASC, strt_dt ASC)
    AND bloom_filter_fp_chance = 0.01
    AND caching = '{"keys":"ALL", "rows_per_partition":"NONE"}'
    AND comment = ''
    AND compaction = {'class': 'org.apache.cassandra.db.compaction.SizeTieredCompactionStrategy'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND dclocal_read_repair_chance = 0.1
    AND default_time_to_live = 0
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';

