    legaldescunit text,
    located_in text,
    lot_size int,
    mannerheld text,
    manufactured_home_condition int,
    manufactured_home_length int,
    manufactured_home_make text,
    manufactured_home_model text,
    manufactured_home_vin text,
    manufactured_home_width int,
    manufactured_manufact_addr text,
    manufactured_manufact_city text,
    manufactured_manufact_name text,
    manufactured_manufact_phone text,
    manufactured_manufact_state text,
    manufactured_manufact_zip text,
    manufacturer_name text,
    number_of_baths int,
    number_of_bedrooms int,
    number_of_floors int,
    other_housingexp text,
    other_improvements decimal,
    parkingspaces int,
    pipeline_flag text,
    prop_addr text,
    prop_addr2 text,
    prop_addr_not_provided text,
    prop_city text,
    prop_county text,
    prop_mailing_address1 text,
    prop_mailing_address2 text,
    prop_mailing_address_city text,
    prop_mailing_address_different text,
    prop_mailing_address_state text,
    prop_mailing_address_zip text,
    prop_state text,
    prop_title_category_type text,
    prop_units int,
    prop_zip text,
    property_type int,
    propwillbe int,
    purpexplain text,
    purploan int,
    reo_recordid text,
    scrubbedaddr1 text,
    scrubbedaddr2 text,
    scrubbedcity text,
    scrubbedcounty text,
    scrubbedstate text,
    scrubbedzip text,
    seller_acquisition_price decimal,
    seller_acquistion_date timestamp,
    snap_dt timestamp,
