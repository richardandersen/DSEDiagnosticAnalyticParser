CREATE KEYSPACE metrics WITH replication = {'class': 'NetworkTopologyStrategy', 'us-east_01': '3', 'us-west-2_02': '3'}  AND durable_writes = true;

CREATE TABLE metrics.chronos_stat_count (
    job_name text,
    task_id text,
    elements_processed counter,
    PRIMARY KEY (job_name, task_id)
) WITH CLUSTERING ORDER BY (task_id ASC)
    AND bloom_filter_fp_chance = 0.1
    AND caching = '{"keys":"ALL", "rows_per_partition":"NONE"}'
    AND comment = ''
    AND compaction = {'class': 'org.apache.cassandra.db.compaction.LeveledCompactionStrategy'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND dclocal_read_repair_chance = 0.1
    AND default_time_to_live = 0
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';

CREATE TABLE metrics.chronos (
    id text,
    ts timestamp,
    attempt int,
    is_failure boolean,
    job_name text,
    job_owner text,
    job_parents set<text>,
    job_schedule text,
    message text,
    slave_id text,
    task_state text,
    PRIMARY KEY (id, ts)
) WITH CLUSTERING ORDER BY (ts ASC)
    AND bloom_filter_fp_chance = 0.1
    AND caching = '{"keys":"ALL", "rows_per_partition":"NONE"}'
    AND comment = ''
    AND compaction = {'class': 'org.apache.cassandra.db.compaction.LeveledCompactionStrategy'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND dclocal_read_repair_chance = 0.1
    AND default_time_to_live = 0
    AND gc_grace_seconds = 864000
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';
CREATE INDEX chronos_job_name_idx ON metrics.chronos (job_name);

